{
  "expo": {
    "name": "RObiStream",
    "slug": "movie-app-fixed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/playstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "robistream",
    "sdkVersion": "54.0.0",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robistream.app"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.robistream.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "robistream"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icons/playstore.png"
    }
  }
}
